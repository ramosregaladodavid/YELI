package creacional.prototype;
public class ProductoConcreto implements ProductoPrototipo { private String nombre; private double precio;
    
public ProductoConcreto(String nombre, double precio) {         this.nombre = nombre;         this.precio = precio;     }
    @Override
    public ProductoPrototipo clonar() { return new ProductoConcreto(nombre, precio);    }
    
    public void mostrar() {         System.out.println("Producto: " + nombre + " - S/ " + precio);     }
    }
